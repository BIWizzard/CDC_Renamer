{
  "name": "cdc-file-renamer",
  "version": "1.0.0",
  "description": "CDC File Renamer - Change Data Capture File Management Tool",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --ext .ts,.tsx .",
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "react-build": "webpack --mode production",
    "react-start": "webpack serve --mode development"
  },
  "keywords": [
    "electron",
    "react",
    "cdc",
    "file-renamer",
    "powershell"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "electron-squirrel-startup": "^1.0.0",
    "node-powershell": "^5.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.63",
    "@electron-forge/maker-deb": "^6.0.0-beta.63",
    "@electron-forge/maker-rpm": "^6.0.0-beta.63",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.63",
    "@electron-forge/maker-zip": "^6.0.0-beta.63",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.85",
    "@types/react-dom": "^17.0.26",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "autoprefixer": "^10.4.0",
    "css-loader": "^7.1.2",
    "electron": "^16.0.0",
    "eslint": "^8.2.0",
    "eslint-plugin-react": "^7.27.0",
    "html-webpack-plugin": "^5.6.3",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "style-loader": "^4.0.0",
    "tailwindcss": "^2.2.19",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.9.5",
    "webpack": "^5.99.6",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/assets/icons/icon.png"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "cdc_file_renamer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
